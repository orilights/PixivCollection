{
  "name": "pixiv-collection",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vue-tsc && vite build",
    "dev": "vite --host",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "pinia": "^2.0.32",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@types/node": "^18.14.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.14.4",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "vue/component-tags-order": [
        "error",
        {
          "order": [
            "template",
            "script",
            "style"
          ]
        }
      ]
    }
  }
}
